{
    "name": "axguowen/think-cdnclient",
    "description": "Simple CDN Client For ThinkPHP",
    "keywords": [
        "thinkphp",
        "cdn",
        "client"
    ],      
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "axguowen",
            "email": "axguowen@qq.com"
        }
    ],
    "require": {
        "topthink/framework": "^6.1||^8.0",
        "axguowen/baidubce-cdn": "^2.0||dev-master",
        "axguowen/ctyun-cdn": "^1.0||dev-master",
        "tencentcloud/cdn": "^3.0",
        "alibabacloud/cdn-20180510": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "think\\": "src"
        }
    },
    "extra": {
        "think": {
            "config": {
                "cdnclient": "src/cdnclient/config.php"
            }
        }
    }
}
